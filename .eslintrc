{
	"extends": "@wpquark",
	"globals": {
		"graphql": true
	},
	"plugins": [
        "markdown"
    ],
	"rules": {
		"react/prefer-stateless-function": 0,
		"react/jsx-filename-extension": 0,
		"jsx-a11y/anchor-is-valid": 0,
		"import/no-unresolved": [2, {
			"ignore": ["^(~)(.*)"]
		}],
		"import/no-extraneous-dependencies": 0
	}
}
